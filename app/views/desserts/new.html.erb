<p>this is for a new dessert</p>

<%= form_for @new_dessert, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <%= file_field_tag 'picture' %>
  <%= f.text_field :name, :required => true %></br>
  <%= f.text_area :description, size: "60x12" %></br>
  <%= f.submit "Create" %>
<% end %>