
<p>Browse our selection for delivery in the Chicagoland area.</p>

<%@desserts.each do |dessert|%>
  <%= link_to dessert_path(dessert.id) do %>
    <div class="dessert_list">
      <%=dessert.name%></br>
      <%=image_tag dessert.pictures[0].picture_url, class: "sample_image", size: "120"%>
    </div>
  <%end%>
<%end%>